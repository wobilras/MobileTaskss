<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/firstFragment">

    <fragment
        android:id="@+id/firstFragment"
        android:name="com.example.taskss.ui.FirstFragment"
        android:label="FirstFragment"
        tools:layout="@layout/fragment_first">
        <action
            android:id="@+id/action_firstFragment_to_registration"
            app:destination="@id/registration" />
        <action
            android:id="@+id/action_firstFragment_to_menuFragment"
            app:destination="@id/menuFragment" />
    </fragment>
    <fragment
        android:id="@+id/secondFragment2"
        android:name="com.example.taskss.ui.SecondFragment"
        android:label="SecondFragment"
        tools:layout="@layout/fragment_second">
        <action
            android:id="@+id/action_secondFragment2_to_chooseCountryFragment"
            app:destination="@id/chooseCountryFragment" />
        <action
            android:id="@+id/action_secondFragment2_to_fragmentResview"
            app:destination="@id/fragmentResview" />
        <argument
            android:name="Login"
            app:argType="string"
            android:defaultValue="User" />
        <action
            android:id="@+id/action_secondFragment2_to_firstFragment"
            app:destination="@id/firstFragment" />
    </fragment>
    <fragment
        android:id="@+id/chooseCountryFragment"
        android:name="com.example.taskss.ui.ChooseCountryFragment"
        android:label="fragment_choose"
        tools:layout="@layout/fragment_choose" />
    <fragment
        android:id="@+id/fragmentResview"
        android:name="com.example.taskss.ui.FragmentResview"
        android:label="fragment_resview"
        tools:layout="@layout/fragment_resview" >
        <action
            android:id="@+id/action_fragmentResview_to_carInfoFragment"
            app:destination="@id/carInfoFragment" />
    </fragment>
    <fragment
        android:id="@+id/carInfoFragment"
        android:name="com.example.taskss.ui.CarInfoFragment"
        android:label="CarInfoFragment"
        tools:layout="@layout/fragmet_car_info">
        <action
            android:id="@+id/action_carInfoFragment_to_reservedCar"
            app:destination="@id/reservedCar" />
    </fragment>
    <fragment
        android:id="@+id/reservedCar"
        android:name="com.example.taskss.ui.ReservedCar"
        android:label="fragment_reserved_car"
        tools:layout="@layout/fragment_reserved_car" />
    <fragment
        android:id="@+id/registration"
        android:name="com.example.taskss.ui.Registration1"
        android:label="fragment_registration"
        tools:layout="@layout/fragment_registration1" >
        <action
            android:id="@+id/action_registration_to_registration2"
            app:destination="@id/registration2" />
    </fragment>
    <fragment
        android:id="@+id/registration2"
        android:name="com.example.taskss.ui.Registration2"
        android:label="fragment_registration2"
        tools:layout="@layout/fragment_registration2" >
        <action
            android:id="@+id/action_registration2_to_registration3"
            app:destination="@id/registration3" />
    </fragment>
    <fragment
        android:id="@+id/registration3"
        android:name="com.example.taskss.ui.Registration3"
        android:label="fragment_registration3"
        tools:layout="@layout/fragment_registration3" />
    <fragment
        android:id="@+id/menuFragment"
        android:name="com.example.taskss.ui.MenuFragment"
        android:label="fragment_menu"
        tools:layout="@layout/fragment_menu" >
        <action
            android:id="@+id/action_menuFragment_to_secondFragment2"
            app:destination="@id/secondFragment2" />
        <action
            android:id="@+id/action_menuFragment_to_fragmentResview"
            app:destination="@id/fragmentResview" />
        <action
            android:id="@+id/action_menuFragment_to_aboutTheApp"
            app:destination="@id/aboutTheApp" />
    </fragment>
    <fragment
        android:id="@+id/aboutTheApp"
        android:name="com.example.taskss.ui.AboutTheApp"
        android:label="fragment_about_the_app"
        tools:layout="@layout/fragment_about_the_app" />
</navigation>